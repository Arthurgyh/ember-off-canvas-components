{
  "name": "ember-off-canvas-components",
  "version": "0.2.1",
  "description": "A micro library of [Ember.js] components that interact to create an Off-Canvas user interface.",
  "main": "index.js",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "start": "ember server",
    "build": "npm run clean; ember build",
    "sass": "./node_modules/.bin/node-sass ./addon/styles/scss/main.scss ./vendor/ember-off-canvas-components.css",
    "clean": "rm -fr ./dist ./vendor/*.css*",
    "test": "ember try:testall"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pixelhandler/ember-off-canvas-components.git"
  },
  "bugs": {
    "url": "https://github.com/pixelhandler/ember-off-canvas-components/issues"
  },
  "homepage": "https://github.com/pixelhandler/ember-off-canvas-components",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Bill Heaton",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.0.2",
    "ember-cli": "0.2.3",
    "ember-cli-app-version": "0.3.3",
    "ember-cli-content-security-policy": "0.4.0",
    "ember-cli-dependency-checker": "0.0.8",
    "ember-cli-ic-ajax": "0.1.1",
    "ember-cli-inject-live-reload": "^1.3.0",
    "ember-cli-qunit": "0.3.10",
    "ember-cli-uglify": "1.0.1",
    "ember-data": "1.0.0-beta.16.1",
    "ember-export-application-global": "^1.0.2",
    "ember-disable-prototype-extensions": "^1.0.0",
    "ember-try": "0.0.4",
    "node-sass": "^2.0.0-beta"
  },
  "keywords": [
    "ember-addon",
    "emberjs",
    "components",
    "off-canvas"
  ],
  "dependencies": {
    "ember-cli-htmlbars": "0.7.6",
    "ember-cli-babel": "^5.0.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}

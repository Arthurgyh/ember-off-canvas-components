    "node": ">= 0.10.0"
  },
  "author": "Bill Heaton",
  "license": "MIT",
  "devDependencies": {
    "broccoli": "^0.13.3",
    "broccoli-asset-rev": "^2.0.0",
    "broccoli-cli": "0.0.1",
    "broccoli-ember-hbs-template-compiler": "^1.7.0",
    "broccoli-es3-safe-recast": "^1.0.0",
    "broccoli-es6-concatenator": "^0.1.11",
    "broccoli-filter": "^0.1.10",
    "broccoli-funnel": "^0.1.7",
    "broccoli-merge-trees": "^0.2.1",
